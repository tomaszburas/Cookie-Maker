<div class="summary-center-container">
    {{#if cookieData.base}}
        <div class="summary-box-left">
            <h1 class="big-txt">Summary</h1>
        </div>
        <div class="summary-box-right">
            <h1 class="big-txt">Preview cookie</h1>
